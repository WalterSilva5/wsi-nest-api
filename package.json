{
  "name": "wsi-nest-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "GPLv3",
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "engines": {
    "npm": ">= 9.8.1",
    "node": ">=16.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "getVersion": "echo $npm_package_version",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:test": "dotenv -e .env.test -- prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "start:prod": "node dist/src/main",
    "lint": "prettier --write \"src/**/*.{ts,js,json}\" && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "generate:module": "python ./module-generator/generate-module.py",
    "test": "jest --config ./test/jest.config.js --verbose",
    "test:silent": "jest --config ./test/jest.config.js --silent --verbose=false --detectOpenHandles",
    "test:watch": "jest --config ./test/jest.config.js --watch",
    "test:cov": "jest --config ./test/jest.config.js --coverage",
    "test:e2e:watch": "jest --watch ./test/jest-e2e.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "docker:up": "docker compose up -d && docker compose run --rm wait-for-db >/dev/null 2>&1",
    "docker:down": "docker compose down",
    "conf:test:e2e": "npm run docker:up && npm run prisma:migrate:test",
    "test:e2e": "npm run conf:test:e2e && dotenv -e .env.test -- jest --config ./test/jest-e2e.json --detectOpenHandles $SINGLE_FILE && npm run docker:down",
    "prepare": "husky install"
  },
  "dependencies": {
    "@asyncapi/generator": "1.10.14",
    "@nestjs/axios": "3.0.0",
    "@nestjs/bull": "10.0.0",
    "@nestjs/cli": "10.0.0",
    "@nestjs/common": "8.0.0",
    "@nestjs/config": "2.1.0",
    "@nestjs/core": "8.0.0",
    "@nestjs/jwt": "10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.0.0",
    "@nestjs/platform-socket.io": "10.0.0",
    "@nestjs/schedule": "3.0.3",
    "@nestjs/schematics": "8.0.2",
    "@nestjs/serve-static": "4.0.0",
    "@nestjs/swagger": "6.1.3",
    "@nestjs/testing": "8.0.0",
    "@nestjs/websockets": "10.0.0",
    "@prisma/client": "4.8.0",
    "@scarwar/winston-rabbitmq": "3.0.0-beta2",
    "archiver": "7.0.1",
    "axios": "1.4.0",
    "axios-retry": "^4.5.0",
    "bcrypt": "5.0.1",
    "bull": "4.11.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "csv-writer": "1.6.0",
    "csvtojson": "2.0.10",
    "dataobject-parser": "1.2.22",
    "dotenv-cli": "7.3.0",
    "global": "4.4.0",
    "i": "^0.3.7",
    "ioredis": "5.3.2",
    "jest-mock-axios": "4.7.3",
    "joi": "17.11.0",
    "moment": "2.25.3",
    "moment-timezone": "0.5.31",
    "npm": "^10.8.3",
    "passport": "0.6.0",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "prisma": "4.8.0",
    "prisma-pagination": "0.2.3",
    "ramda": "0.28.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "sib-api-v3-sdk": "8.5.0",
    "socket.io": "4.7.1",
    "streamifier": "0.1.1",
    "unzipper": "0.12.1",
    "uuid": "^10.0.0",
    "winston": "3.14.2"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@types/archiver": "5.3.2",
    "@types/bcrypt": "5.0.0",
    "@types/cron": "2.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "29.5.11",
    "@types/multer": "1.4.7",
    "@types/node": "16.18.38",
    "@types/passport-google-oauth20": "2.0.11",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/stream-buffers": "3.0.4",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.52.1-alpha.1",
    "@typescript-eslint/parser": "5.52.1-alpha.1",
    "axios-mock-adapter": "1.22.0",
    "eslint": "8",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-import-helpers": "1.2.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "nodemon": "2.0.20",
    "prettier": "2.3.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.2.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.(test|spec)\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "moduleNameMapper": {
      "src/(.*)$": "<rootDir>/src/$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
